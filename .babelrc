{
    "presets": [
        ["env", {
            "modules": false,
            "targets": {
                "node": "current",
                "browsers": [
                    "last 2 versions",
                    "ie >= 9"
                ]
            }
        }]
    ],
    "plugins": [
        ["babel-plugin-transform-builtin-extend", {
            "globals": ["Error"]
        }]
    ],
    "env": {
        "test": {
            "presets": [],
            "plugins": ["transform-es2015-modules-commonjs"]
        }
    }
}
